{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock",
      "check-postbrace"
    ],
    "await-promise": [true, "Bluebird"],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "quote-props": ["error", "as-needed", {
      "numbers": true
    }],
    "no-console": [true, "never"],
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}
